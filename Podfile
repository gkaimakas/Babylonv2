# Uncomment the next line to define a global platform for your project
platform :ios, '12.0'

target 'Babylon' do
  use_frameworks!
  
  pod 'Alamofire'
  pod 'DifferenceKit'
  pod 'SnapKit'
  pod 'ReactiveCocoa'
  pod 'ReactiveSwift'
  pod 'Swinject'

  
  # Pods for Babylon
  
  target 'BabylonTests' do
    inherit! :search_paths
    # Pods for testing
  end
  
  target 'BabylonUITests' do
    inherit! :search_paths
    # Pods for testing
  end
  
end

target 'BabylonCommon' do
  use_frameworks!
  
  pod 'ReactiveSwift'
  # Pods for BabylonCommon
  
  target 'BabylonCommonTests' do
    inherit! :search_paths
    # Pods for testing
  end
  
end

target 'BabylonModels' do
  use_frameworks!
  
  pod 'Alamofire'
  pod 'ReactiveSwift'
  
  target 'BabylonModelsTests' do
    inherit! :search_paths
    # Pods for testing
  end
  
end

target 'BabylonViewModels' do
  use_frameworks!
  
  pod 'ReactiveSwift'
  
  target 'BabylonViewModelsTests' do
    inherit! :search_paths
    # Pods for testing
  end
  
end

target 'BabylonViews' do
  use_frameworks!
  
  pod 'DifferenceKit'
  pod 'SnapKit'
  pod 'ReactiveCocoa'
  pod 'ReactiveSwift'
  
  target 'BabylonViewsTests' do
    inherit! :search_paths
    # Pods for testing
  end
  
end
